    "@babel/register": "^7.0.0",
    "acorn": "^6.4.1",
    "autoprefixer": "^9.4.8",
    "babel-loader": "^8.0.5",
    "babel-register": "^6.26.0",
    "bootstrap": "^4.3.1",
    "commander": "^3.0.0",
    "css-loader": "^2.1.0",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "dotenv": "^6.2.0",
    "faker": "^4.1.0",
