{
  "name": "brasid-task-manager",
  "version": "1.0.0",
  "description": "student's redis-like task manager web-app",
  "engines": {
    "node": "11.9",
    "npm": "6.7.0"
  },
  "main": "index.js",
  "bin": "./bin/server",
  "scripts": {
    "test": "jest",
    "server:hot": "npx --env development nodemon ./bin/server server",
    "server": "node ./bin/server server",
    "build": "npx webpack -p --env production && npx sequelize db:migrate",
    "postinstall": "npm run build",
    "repl": "npx ./bin/server console"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afufev/project-lvl4-s383.git"
  },
  "keywords": [
    "app",
    "node",
    "js",
    "taskmanager"
  ],
  "author": "alexander fufaev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afufev/project-lvl4-s383/issues"
  },
  "homepage": "https://github.com/afufev/project-lvl4-s383#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/*.js",
      "routes/*.js",
      "models/*.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/__fixtures__",
      "/__tests__/helpers"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
